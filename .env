# Server configuration
PORT=5000

# PostgreSQL database connection
# Format: postgresql://username:password@host:port/database

# JWT secret for authentication
JWT_SECRET=super_secret_jwt_key_for_investro_app

# Session secret (used if using express-session or similar)
SESSION_SECRET=super_secret_session_key_for_investro_app

# File upload directory
UPLOAD_DIR=server/uploads

# Default admin credentials
ADMIN_USERNAME=admin
ADMIN_PASSWORD=AdminStrongPass123

# Optional: Environment mode (development or production)
NODE_ENV=development
DATABASE_URL=postgresql://investro_user:MySuperStrongPassword123@127.0.0.1:5432/investro_db
